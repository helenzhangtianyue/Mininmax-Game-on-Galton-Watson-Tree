    "def partition ( number ):\n",
    "    answer = set ()\n",
    "    answer .add (( number , ))\n",
    "    for x in range (1, number ):\n",
    "        for y in partition ( number - x):\n",
    "            answer . add( tuple ( sorted ((x, ) + y )))\n",
    "    return list ( answer )"
